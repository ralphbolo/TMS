<h3>Create a new team</h3>



	<div class="row">
		<div class="col-md-6 col-md-offset-3">

				<%= form_for @team  do |f| %>
				<div class="field form-group">
				  <%= f.label :teamName %>
				  <%= f.text_field :teamName, :class => 'form-control' %>
				</div>

				<div class="field form-group">
				  <p>liason</p>
				  <%= f.collection_select :provider_id, @students, :id, :email, include_blank: true %>
				</div>





				<%= f.submit %>
				<% end %>

		</div>
	</div>
